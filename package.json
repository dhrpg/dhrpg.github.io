{
  "name": "dhrpg",
  "version": "0.1.0",
  "description": "DH RPG webpage using Hylia and Eleventy",
  "main": "index.js",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@tbranyen/jsdom": "^13.0.0",
    "concurrently": "^7.0.0",
    "eleventy-plugin-toc": "^1.1.5",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "json-to-scss": "^1.5.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "sass": "^1.26.3",
    "semver": "^6.3.0",
    "slugify": "^1.4.0",
    "stalfos": "github:hankchizljaw/stalfos#c8971d22726326cfc04089b2da4d51eeb1ebb0eb"
  },
  "devDependencies": {
    "@erquhart/rollup-plugin-node-builtins": "^2.1.5",
    "bl": "^3.0.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.2.1",
    "make-dir-cli": "^3.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "scripts": {
    "sass:tokens": "npx json-to-scss src/_data/tokens.json src/scss/_tokens.scss",
    "sass:process": "npm run sass:tokens && sass src/scss/global.scss src/_includes/assets/css/global.css --style=compressed",
    "start": "concurrently \"npm run sass:process -- --watch\"  \"chokidar \\\"src/_includes/**\\\" -c \\ \"npm run serve\"",
    "serve": "cross-env ELEVENTY_ENV=development npx eleventy --serve",
	"build": "eleventy",
    "production": "npm run sass:process && npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hankchizljaw/hylia.git"
  },
  "keywords": [],
